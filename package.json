    "autoprefixer": "^10.4.0",
    "bootstrap": "^5.1.3",
    "electron-squirrel-startup": "^1.0.0",
    "graphql": "^15.7.2",
    "ng-apexcharts": "^1.5.12",
    "ngx-countdown": "^12.0.1",
    "ngx-markdown": "^13.0.0",
    "ngx-matomo-v9": "^0.4.0",
    "ngx-spinner": "^13.0.0",
    "ngx-toastr": "^14.2.1",
    "postcss": "^8.3.11",
    "rxjs": "^7.4.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^13.0.0",
